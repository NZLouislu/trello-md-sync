{
  "name": "trello-md-sync-examples",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "md": "node -r ts-node/register ./md-to-trello.ts",
    "md:debug": "set LOG_LEVEL=debug&& node -r ts-node/register ./md-to-trello.ts",
    "md:json": "set LOG_JSON=1&& node -r ts-node/register ./md-to-trello.ts",
    "trello": "node -r ts-node/register ./trello-to-md.ts",
    "trello:debug": "set LOG_LEVEL=debug&& node -r ts-node/register ./trello-to-md.ts",
    "test": "mocha -r ts-node/register \"__tests__/**/*.test.ts\""
  },
  "devDependencies": {
    "mocha": "^10.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}